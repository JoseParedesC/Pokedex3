{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/api/*"]
    },
    "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/admin",
      "allowedRoles": ["admin"]
    },
    {
      "route": "/*",
      "headers": {
        "permissions-policy": "geolocation=(self), microphone=(), camera=(), fullscreen=(self), payment=()"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}